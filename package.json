{
  "type":"module",
  "name": "selenium",
  "version": "1.0.0",
  "description": "Seleniumv webdriver",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test:e2e:coverage": "cross-env BROWSER=chrome npx jest --coverage --detectOpenHandles TC.test.js",
    "test:e2e:firefox": "cross-env BROWSER=firefox npx jest --reporters jest-html-reporters --detectOpenHandles TC.test.js",
    "test:e2e:chrome": "cross-env BROWSER=chrome npx jest  /test/TC.test.js  --reporters jest-html-reporters",
    "test:e2e:edge": "cross-env BROWSER=edge npx jest TC.test.js",
    "test:e2e:env": "cross-env BROWSER=chrome cross-env URL=staging_env npx jest  /test/TC.test.js"
  },
  "author": "Nikita Koni",
  "license": "ISC",
  "dependencies": {
    "@microsoft/edge-selenium-tools": "^3.6.1",
    "chromedriver": "^90.0.0",
    "jest-html-reporters": "^2.1.6",
    "ms-chromium-edge-driver": "^0.4.2",
    "selenium-standalone": "^6.23.0"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "geckodriver": "^1.22.3",
    "jest": "^26.6.3",
    "jest-html-reporter": "^3.3.0",
    "pm2": "^4.5.6",
    "selenium-webdriver": "3.6.0",
    "webdriver-manager": "^12.1.8"
  }
}
